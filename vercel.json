{
  "buildCommand": "echo 'Static site - no build needed'",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "framework": "html",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
